<h1>Listing certificacions</h1>

<table>
  <thead>
    <tr>
      <th>Titulo certificacion</th>
      <th>Descripcion certificacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @certificacions.each do |certificacion| %>
      <tr>
        <td><%= certificacion.titulo_certificacion %></td>
        <td><%= certificacion.descripcion_certificacion %></td>
        <td><%= link_to 'Show', certificacion %></td>
        <td><%= link_to 'Edit', edit_certificacion_path(certificacion) %></td>
        <td><%= link_to 'Destroy', certificacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Certificacion', new_certificacion_path %>
