<!DOCTYPE html>
<html>
<head>
  <title>SyTConsulting</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag :application %>
  <%= csrf_meta_tags %>
</head>

<div id="notice">
      <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, :id => "flash_#{name}" %>
      <% end %>
 </div>

<%= yield %>

</html>