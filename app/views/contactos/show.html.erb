<div class="container" style="margin-top:60px;">
<div class="jumbotron">
<p id="notice"><%= notice %></p>

<p>
  <strong>Nombre:</strong>
  <%= @contacto.nombre %>
</p>

<p>
  <strong>Correo electronico:</strong>
  <%= @contacto.correo_electronico %>
</p>

<p>
  <strong>Telefono:</strong>
  <%= @contacto.telefono %>
</p>

<p>
  <strong>Mensaje:</strong>
  <%= @contacto.mensaje %>
</p>

<br>
<br>
<%= link_to 'Borrar', @contacto, :class=>'btn btn-warning' ,method: :delete, data: { confirm: 'Esta seguro que desea borrar este mensaje?' } %>
<%= link_to 'Atras', :back, :class=>"btn btn-success" %>
</div>
</div>