		</div>
		<div class="timeline-body">
			<p class="text-muted">
				<%= acontecimiento.descripcion_acontecimiento %>
			</p>
		</div>
	</div>
</li>
<% end %>

<li class="timeline-inverted">
<div class="timeline-image" style="background-color:#6CB1FA;">
<h4>Se parte 
<br>de nuestra
<br>historia</h4>
</div>
</li>
</ul>
</div>
</div>
</div>
</section>
<!-- CONTACTOS -->
<section id="contact">
<div class="container">
<div class="row">
<div class="col-lg-12 text-center">
<h2 class="section-heading">Contactanos</h2>
<h3 class="section-subheading text-muted">Dejanos tu mensaje</h3>
</div>
</div>
<div class="row">
<div class="col-lg-12">
<%= form_for(@contacto) do |f| %>
  <% if @contacto.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@contacto.errors.count, "error") %> prohibited this contacto from being saved:</h2>

      <ul>
      <% @contacto.errors.full_messages.each do |cont| %>
        <li><%= cont %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
	<div class="row">
	<div class="col-md-6">
	<div class="form-group">
		<h3 style="color:#659DEE;">Nombre*</h3>
    	<%= f.text_field :nombre, :size => "60x1", :maxlength => 30, :placeholder => "Ingrese su nombre" %>
	</div>
	<div class="form-group">
		<h3 style="color:#659DEE;">Correo electronico*</h3>
	    <%= f.text_field :correo_electronico, :size => "60x1", :maxlength => 50, :placeholder => "Ingrese su correo electronico" %>
	</div>
	<div class="form-group">
		<h3 style="color:#659DEE;">Telefono*</h3>
	    <%= f.text_field :telefono, :size => "60x1", :maxlength => 30, :placeholder => "Ingrese telefono" %>
	</div>
	</div>
	<div class="col-md-6">
	<div class="form-group">
		<h3 style="color:#659DEE;">Mensaje*</h3>
    	<%= f.text_area :mensaje , :size => "60x14", :maxlength => 500, :placeholder => "Ingrese su mensaje" %>
	</div>
	</div>
	<div class="clearfix"></div>
	<div class="col-lg-12 text-center">
	<div id="success">
		 <button type="submit" class="btn btn-xl">Enviar Mensaje</button>
	</div>
	    
	</div>
	</div>
<% end %>
</div>
</div>
</div>
</section>
