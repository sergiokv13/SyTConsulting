<!-- La empresa -->
<section id="la_empresa">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 text-center">
				<h2 class="section-heading">
					La Empresa
				</h2>
			</div>
		</div>
		<div class="container row text-center well">
			<strong> :: Eficiencia </strong> <br/>
			<strong> :: Eficacia </strong> <br/>
			<strong> :: Puntualidad </strong> <br/>
			<strong> :: Flexibilidad </strong> <br/>
			<strong> :: Trabajo certificado </strong> <br/>
			<strong> :: Calidad de Servicio	</strong> <br/>
		</div>

		<div class="row">
		  <div class="col-md-5 text-center well">
		  	<h3>Misión</h3>
					Ser empresa de prestigio en el área de consultoría tecnológica, abarcando un mercado nacional e internacional, brindando soluciones eficientes a sus clientes a través de un trabajo altamente certificado. 
		  </div>
		  <div class="col-md-2"></div>
		  <div class="col-md-5 text-center well">
		  		<h3>Visión</h3>
				 	Ofrecer servicios de consultoría y soluciones al cliente del ámbito industrial y tecnológico a nivel local y nacional, a través de un trabajo de calidad realizado por un personal altamente calificado y certificado.
		  </div>
		</div>
		<div class="row">
		  <div class="col-md-4"></div>
		  <div class="col-md-4 well">
		  <h3 class="text-center">Descripcion general de la empresa</h3>
			   &#8226 Empresa establecida en Cochabamba – Bolivia<br/>
			   &#8226 Fundador Mgr. Luis Alberto Molina<br/>
			   &#8226 Puntualidad y eficiencia avaladas por 20 años de experiencia en el mercado de la industria y la tecnología.<br/>
			   &#8226 Posicionamiento solido en el ámbito local y nacional.<br/>
			   &#8226 Experiencia en el manejo, implementación y mantenimiento de las marcas más prestigiosas, como ser: 3M, etc<br/>
			   &#8226 Valorado por muchas empresas de prestigio en el medio, el trabajo realizado por la empresa refleja una organización dedicada y eficiente, con métodos de trabajo reactivos, versátiles y transparentes.<br/>
			   &#8226 Calidad de Servicio<br/>
			   &#8226 Trabajos garantizados por 1 año<br/>
			   &#8226 Gerente General - <strong>S&T Consulting</strong>: Luis Alberto Molina (27 años de experiencia dentro de empresas de gran envergadura engerencia de áreas tecnológicas, y administración)<br/>

		  </div>
		  <div class="col-md-4"></div>
		</div>
	</div>

</section>


<!-- Services Section -->
<section id="services">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 text-center">
				<h2 class="section-heading">
					Servicios
				</h2>
				<h3 class="section-subheading text-muted well">
					<%= @informacion.contenido_informacion %>
				</h3>
			</div>
		</div>
		<div class="row text-center">
			<% @servicios.each do |servicio| %>
			<div class="col-md-4">
					<img src="<%= servicio.foto.url %>" width="150px;">
				<h4 class="service-heading">
					<a href="#" data-toggle="modal" data-target="#detalle_servicio" data-keyboard="true" style="text-decoration:none;">
						<%= servicio.nombre_servicio%>
					</a>
				</h4>
				<p class="text-muted">
					<%= servicio.descripcion_servicio%>
				</p>
			</div>

			<div class="modal fade" id="detalle_servicio" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel2" aria-hidden="true">
	            <div class="modal-dialog modal-lg">
	                <div class="modal-content">
			            <div class="modal-header">
			                    <strong>
			                    	<h1>
			                    		<%= servicio.nombre_servicio%>
			                    	</h1>
			                    </strong>
			            </div>
			            <div class="modal-body">
			                <ul>
		                      	<h3>
		                      		<%= servicio.descripcion_servicio %>
		                      	</h3>
			                </ul>
			            </div>
			        </div>
	            </div>
        	</div>

			<% end %>
		</div>
	</div>
</section>
