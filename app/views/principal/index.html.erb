<ul class="timeline">
<% @acontecimientos.each do |acontecimiento| %>
<li>
	<div class="timeline-image">
		<img class="img-circle img-responsive" src=<%= acontecimiento.foto.url %> alt="">
	</div>
	<div class="timeline-panel">
		<div class="timeline-heading">
			<h4>
				<%= acontecimiento.periodo_acontecimiento %>
