<!-- About Section -->
<section id="about">
<div class="container">
<div class="row">
<div class="col-lg-12 text-center">
<h2 class="section-heading">Acontecimientos</h2>
<h3 class="section-subheading text-muted">Conoce mas sobre los acontecimientos que marcaron nuestra empresa</h3>
</div>
</div>
<div class="row">
